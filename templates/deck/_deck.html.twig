<div class="borderDeck">List of cards : 
    {% for deckCard in deck.deckCards %}
        {% if deckCard.nbr > 0 %}
            {% for i in 1..deckCard.nbr %}
                <li class="deck">
                    <a href="{{ path('remove_card_in_deck' , {'id_card' :deckCard.card.id ,'id_deck' : deckCard.deck.id }) }}">
                        {{deckCard.card.nom}}     
                    </a> 
                </li>
            {% endfor %}
        {% endif %}
    {% endfor %}
</div>
